{
	"name": "kit-auth0-nextauth",
	"version": "2.0.1",
	"author": "Aakash Goplani",
	"description": "Authentication in SvelteKit with Auth0 OAuth Provider and SvelteKitAuth",
	"scripts": {
		"dev": "vite dev --port 4000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --write . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"dependencies": {
		"@auth/core": "0.10.0",
		"@auth/sveltekit": "0.3.6"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "3.0.2",
		"@sveltejs/kit": "1.22.3",
		"@typescript-eslint/eslint-plugin": "6.2.0",
		"@typescript-eslint/parser": "6.2.0",
		"eslint": "8.45.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte": "2.32.4",
		"prettier": "3.0.0",
		"prettier-plugin-svelte": "3.0.0",
		"svelte": "4.1.1",
		"svelte-check": "3.4.6",
		"svelte-preprocess": "5.0.4",
		"sass": "1.64.1",
		"tslib": "2.6.1",
		"typescript": "5.1.6",
		"vite": "4.4.7"
	},
	"type": "module"
}
